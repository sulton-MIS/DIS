    function GetListOption(element) {
        var arr = [];
        $('#' + element + ' option').each(function () {
            var objdt = {
                values: $(this).val(),
                texts: $(this).text()
            }
            arr.push(objdt);
        });
        return arr;
    }

    function MappingListOption(element, arr) {
        $('#' + element).html('');

        arr.forEach(function (item, index) {
            $('#' + element).append("<option value=" + item.values + ">" + item.texts + "</option>");
        });
    }

