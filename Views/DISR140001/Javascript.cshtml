    // proses pencarian data
    function searchData(page, size) {
        var id_bundle = $('#searchIDBundle').val();
        var dmc_code = $('#searchDmcCode').val();
        var trans_date = $('#searchDate').val();
        var trans_dateTo = $('#searchDateTo').val();
        var nik = $('#searchNIK').val();
        var opr_gaikan = $('#searchOprGaikan').val();

        Last_Record_Per_Page = $("#paging").val();
        $.ajax({
            type: "POST",
            url: SITE_URL + 'Search_Data',
            async: false,
            data: {
                start: page,
                display: size,
                DATA_ID: $("#data_nm").val(),
                ID_BUNDLE: id_bundle,
                DMC_CODE: dmc_code,
                TRANS_DATE: trans_date,
                TRANS_DATETO: trans_dateTo,
                NIK_GAIKAN: nik,
                OPR_GAIKAN: opr_gaikan
            },
            success: function (data) {
                $("#GridTable").html(data);
                $("#paging").val(Last_Record_Per_Page);

                scroll_and_checkbox();
                cek_perdata();
            }
        });
    }

