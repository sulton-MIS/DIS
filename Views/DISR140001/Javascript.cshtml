    function validationProyekData() {
        

        var ValidationState = true;
        var ValidationMessage = "";

        var ProyekData = {
            JUMLAH_BDL_STD: $("#addJumlahBundleStandar").val()
        }

        var table = document.getElementById("TBL_LIST_DETAIL");
        var totalRowCount = table.rows.length; // 5

        if (totalRowCount > ProyekData.JUMLAH_BDL_STD) {
            $('#QtyBeratUp_PopUp').modal();
            ValidationState = false;
        }
        if (totalRowCount < ProyekData.JUMLAH_BDL_STD) {
            $('#QtyBeratUnder_PopUp').modal();
            ValidationState = false;
            return;
        }

        var rst = {
            ValidationState: ValidationState,
            ValidationMessage: ValidationMessage
        }

        return rst;
    }


