    function validationDataDetail() {
        var ValidationState = true;
        var ValidationMessage = "";

        var table = document.getElementById("TBL_LIST_DETAIL");
        var totalRowCount = table.rows.length;
        var JUMLAH_BDL_STD = $("#addJumlahBundleStandar").val();
        var JUMLAH_BDL_ACT = $("#addJumlahBundleAktual").val();

        if (JUMLAH_BDL_ACT > JUMLAH_BDL_STD || totalRowCount >= JUMLAH_BDL_STD) {
            $('#QtyBeratUp_PopUp').modal();
            ValidationState = false;
        }

        var rst = {
            ValidationState: ValidationState,
            ValidationMessage: ValidationMessage
        }

        return rst;
    }

