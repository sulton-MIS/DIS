    function confirmDeleteJobProyek() {
        var Id = $(_row).find('td:first-child').attr('data-title');

        // remove item array ListDataProd
        var serial = $(_row).find('td:nth-child(5)').text();
        ListDataProd.splice($.inArray(serial, ListDataProd), 1);

        // remove item array ListDataLotto
        var lot_no = $(_row).find('td:nth-child(6)').text();
        ListDataLotto.splice($.inArray(lot_no, ListDataLotto), 1);

        if (parseInt(Id) > 0) {
            $.ajax({
                url: "../DeleteDetail",
                type: "GET",
                data: { ID: Id },
                async: false
            }).done(function (data, textStatus, jqXHR) {
                if (data.Message != "") {
                    toastr.error(data.Message);
                }
                else {
                    toastr.success("Data has been deleted");
                    var name = $(_row).find('td:nth-child(1)').text() + ',' + $(_row).find('td:nth-child(2)').text();
                    _row.remove();
                }
            })
                .fail(function (jqXHR, textStatus, errorThrown) {
                    toastr.error(errorThrown);
                });
        }
        else {
            var name = $(_row).find('td:nth-child(1)').text() + ',' + $(_row).find('td:nth-child(2)').text();
            _row.remove();
        }

        //Change Status Lotto
        var isExist = false;
        var STATUS_LOTTO = "";
        if (ListDataLotto.length > 0) {
            for (var i = 0; i < ListDataLotto.length; i++) {
                if (ListDataLotto[i + 1] == ListDataLotto[i]) {
                    isExist = true;
                    break;
                }
            }

            if (ListDataLotto.length > 1) {
                if (isExist == true) {
                    STATUS_LOTTO = "Non Campuran";
                } else {
                    STATUS_LOTTO = "Campuran";
                }
            }
            else {
                STATUS_LOTTO = "";
            }
        }
        else {
            STATUS_LOTTO = "";
        }

        $("#addStatusLotto").val(STATUS_LOTTO);

        //===== Change Qty Bundle Actual ======
        var qty_actual = document.getElementById("addJumlahBundleAktual").value;
        qty_actual = qty_actual - 1;

        $('#addJumlahBundleAktual').val(qty_actual).trigger("change");

        //====== Change Berat Bundle Actual =====
        var berat_act = document.getElementById("addBeratBundleAktual").value;
        var avg_berat = document.getElementById("addBeratPcsAktual").value;

        berat_act = berat_act - avg_berat;
        $('#addBeratBundleAktual').val(berat_act).trigger("change");

        //===== Change Color Submit Btn =======
        ChangeButton();

    }

