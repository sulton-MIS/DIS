                YIELD_DENKI_GLASS: YIELD_DENKI_GLASS
            },
            success: function (res) {
                if (res.sts == "TRUE") {
                    toastr.success("Data Has Been Save Successfuly");
                    searchData();
                    clear_add();                    
                    $('#modalForm').modal('hide');
                    $('#btncloselookup').click();
                    $('#btnNo').click();
