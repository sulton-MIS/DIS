<style>
    .menu-text-panel {
        position: absolute;
        top: 40%;
        left: 50%;
        transform: translate(-50%, -50%);
        text-align: center;
        vertical-align: middle;
    }

    .menu-panel:hover {
        background-color: rgb(84, 96, 125);
        cursor: pointer;
    }

    hr {
        margin: 0px 25px;
        margin-top: 0px;
        margin-bottom: 0px;
        border-width: 1px 0 0 0;
        border-top: 1px solid #E3E3E3;
    }

    .row {
        margin: 0;
        padding: 0;
    }

    @@media only screen and (max-width: 600px) {
        .menu-panel {
            min-height: 250px;
        }

        .fa-menu-icon {
            font-size: 80px;
            font-weight: 800;
        }

        #menu-container {
            margin: 20px 20px 20px 20px;
            padding: 10px;
        }
    }

    @@media only screen and (min-width: 600px) {
        .menu-panel {
            min-height: 350px;
        }

        .fa-menu-icon {
            font-size: 80px;
            font-weight: 800;
        }

        .menu-text-list {
            font-size: 18px;
        }

        .menu-text-master {
            font-size: 20px;
        }

        #menu-container {
            margin: 20px 20px 20px 20px;
            padding: 10px;
        }
    }

    #title {
        border: 0px solid !important;
        border-bottom: 1px solid #eaeaea !important;
        text-align: center;
    }
        /*#title:hover {
            background: #234893;
            border-bottom: 1px solid # !important;
            text-align: center;
            color: white !important;
        }*/

    #title h4{
        font-family: 'Bookman Old Style';
        color: darkcyan;
    }

</style>

<input id="userLogin" style="display:none" type="text" value="@ViewData["User"]" />
<input id="regLogin" style="display:none" type="text" value="@ViewData["RegNo"]" />

<div id="menu-container" class="container-fluid">
    <div class="container-fluid">
