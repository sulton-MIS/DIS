    function image_check() {
        var ID = $('#ID').val();
        var ID_TB_M_LAPOR = $('#ID_TB_M_LAPOR').val();
        var NO_LAPOR = $('#NO_LAPOR').val();
        var NO_ASSET = $('#NO_ASSET').val();
        var FOTO_NAME = NO_ASSET.trim() + "." + getExt(($('#editImg')[0].files[0].name));

        var sts = true;

        //Check Format Foto
        var FORMAT_FOTO = "";
        if ($('#editImg').val() != "") {
            var NAMA_FOTO = ($('#editImg')[0].files[0].name).split('.');
            FORMAT_FOTO = (NAMA_FOTO[NAMA_FOTO.length - 1]).toUpperCase();
        }

        if (FORMAT_FOTO != 'PNG' && FORMAT_FOTO != 'JPG') {
            toastr.error("Extension wrong, please use '.PNG' Or '.JPG' file!");
            sts = false;
        }

        if (sts == false) {
            return;
        } else {
            if ($('#Edit_Image').val() != "True") {
                toastr.warning("Your Account Doesn't Have Access! Please Contact Administrator!");
                return;
            } else {
                $('#PopupForm_EditImageConfirm').modal();
            }
        }
    }

    function update_image() {
        var ID = $('#ID').val();
        var ID_TB_M_LAPOR = $('#ID_TB_M_LAPOR').val();
        var NO_ASSET = $('#NO_ASSET').val();
        var FOTO_NAME = ID.trim() + "." + getExt(($('#editImg')[0].files[0].name));

        $.ajax({
            type: "POST",
            url: SITE_URL + '../UPDATE_IMAGE_LAPOR_ASSET',
            async: false,
            data: {
                ID: ID,
                NO_ASSET: NO_ASSET,
                FOTO_NAME: FOTO_NAME
            },
            success: function (res) {
                if (res.sts == "TRUE") {
                    //Send Image To Folder
                    if (FOTO_NAME != "") {
                        MoveFoto('#editImg', FOTO_NAME);
                    }
                    toastr.success("Data Has Been Save Successfuly");
                    $('#modalShowImageForm').modal('hide');
                    cancel_edit();
                    jQuery('#btn_search').click();
                } else {
                    toastr.warning(res.message);
                }
            },
            error: function (res) {
                toastr.error(res.Message);
            }
        });
        refreshPage();
    }

