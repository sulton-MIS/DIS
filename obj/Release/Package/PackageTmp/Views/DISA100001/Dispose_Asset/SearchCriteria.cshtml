@using AI070.Models;
@using AI070.Models.Shared;
@using AI070.Models.DISA100001Master;
@{
    List<DISA100001Master> DataWP = (List<DISA100001Master>)ViewData["DataDISA100001"];
    List<DISA100001> Status_Cmb = (List<DISA100001>)ViewData["Status_Cmb"];
    List<DISA100001> Time_Cmb = (List<DISA100001>)ViewData["Time_Cmb"];
    List<DISA100001> ExecTime_Cmb = (List<DISA100001>)ViewData["Time_Cmb"];
    List<DISA100001> Model_type = (List<DISA100001>)ViewData["Model_type"];
    List<ListAssetModel> ListAsset = (List<ListAssetModel>)ViewData["no_asset"];
}

<style>
    #tableSearch {
        border-spacing: 10px;
        border-collapse: separate;
    }
</style>

<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-11" style="margin-top: -13px; margin-left: 10px;">
        <h2>@ViewData["Title1"]</h2>
    </div>
</div>

<div id="formSearch" class="row">
    <div class="col-xs-12 col-sm-12 col-md-12">
        <div class="panel panel-default">
            <div class="panel-body">
                <form role="form" class="form-horizontal" id="searchForm">
                    <div class="row" style="margin-bottom: -25px;margin-top: -10px">
                        <div class="col-md-12">
                            <div class="col-md-3">
                                NO. DISPOSE:
                                <input id="searchNoDispose" type="text" class="form-control" autocomplete="off" />
                            </div>
                            <div class="col-md-3">
                                NO ASSET:
                                <input id="searchNoAsset" type="text" class="form-control" autocomplete="off" />
                            </div>
                            <div class="col-md-3">
                                STATUS APPROVAL:
                                <select id="searchStatusApproval" class="form-control">
                                    <option id="searchStatusApproval" value="" selected="selected"> --ALL-- </option>
                                    <option id="searchStatusApproval" value="PREPARED"> PREPARED </option>
                                    <option id="searchStatusApproval" value="CHECKED"> CHECKED </option>
                                    <option id="searchStatusApproval" value="APPROVED_AMS"> APPROVED AMS </option>
                                    <option id="searchStatusApproval" value="APPROVED_DEPT_HEAD_AMS"> APPROVED DEPT.HEAD AMS </option>
                                    <option id="searchStatusApproval" value="ACKNOWLEDGE"> ACKNOWLEDGE </option>
                                    <option id="searchStatusApproval" value="REJECTED"> REJECTED </option>
                                </select>
                            </div>
                        </div>
                        
                    </div>
                    <br />

                    <div class="row">
                        <div style="margin-bottom: 10px"></div>
                        <div class="col-xs-12 col-sm-12 col-md-4">

                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-8 text-right">
                            <button type="button" class="btn btn-primary btn-xs" id="btn_search" style="width: 80px;"><i class="fa fa-search"></i> Search</button>
                            <button type="button" onclick="clear_search()" class="btn btn-grey btn-xs" id="btn_clear" style="width: 80px;"><i class="fa fa-eraser"></i> Clear</button>
                        </div>
                    </div>

                    <div class="row">
                        <div class="hr hr-10" style="margin-bottom: 10px"></div>
                        <div class="col-xs-12 col-sm-12 col-md-7">
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-5 text-right">
                            <img id="_reloading" src="Content/Bootstrap/img/loading.gif" alt="Please Wait" style="width: 30px; height: 30px; margin-right: 4px; opacity: 0;" />
                            <button type="button" class="btn btn-primary btn-xs" id="btn_add" style="width: 80px;" onclick="showAdd()"><i class="fa fa-plus"></i> New</button>
                            <button type="button" class="btn btn-primary btn-xs" id="btn_edit" style="width: 80px;" onclick=""><i class="fa fa-pencil-square-o"></i> Edit</button>
                            <button type="button" class="btn btn-danger btn-xs" id="btn_Delete" style="width: 80px;" onclick=""><i class="fa fa-trash-o"></i> Delete</button>
                            <button type="button" class="bn btn-success btn-xs" id="btn_download" style="width: 100px;" onclick=""><i class="fa fa-print"></i> Download </button>
                            @*<button type="button" class="btn btn-primary btn-xs" id="btn_new" style="width: 80px;" onclick=""><i class="fa fa-plus"></i> New</button>*@
                            <button type="button" class="btn btn-yellow btn-xs" id="btn_printout" style="width: 120px;" onclick=""><i class="fa fa-print"></i> Export Form</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

