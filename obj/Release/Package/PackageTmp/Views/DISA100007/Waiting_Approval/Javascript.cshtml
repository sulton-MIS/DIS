                $('#display_Nama_Peminjam').hide();
                $('#display_Waktu_Pinjam').hide();
                $('#display_Waktu_Simpan').show();

                $('#addNomorRak').prop('disabled', false);
                $('#addLabelRak').prop('disabled', false);

                $('#addNomorRak').val('');
                $('#addLabelRak').val('---Pilih---');
            }
        });
        $('#addJenisTransaksi').prop('selected', true);
        $('#addJenisTransaksi').trigger('change');
