                        //$('#addDepartment').prop('disabled', true);

                        //Auto selected Bagian (Section)
                        $('#addBagian').val(e.data[0].BAGIAN).trigger("change");
                        var value = $('#addDepartment option:selected').val();
                        if (value != '') {
                            $('#addBagian').removeAttr('disabled');
                            $("#addBagian option").hide();
                            var code = value;
                            $("#addBagian option[area='" + code + "']").prop('disabled', false);
                            $("#addBagian option:not([area='" + code + "'])").prop('disabled', true);

                        }

                        $('#addNomorRak').val(e.data[0].RAK);
                        $('#addLabelRak').val(e.data[0].LABEL_RAK);
                        $('#addMasaSimpan').val(e.data[0].MASA_SIMPAN);
                        $('#addKeterangan').val(e.data[0].KETERANGAN);

                        $('#display_Nama_Peminjam').hide();

                        alert('Data ini sudah ada di List Master Document!')

                    }
                    else {
                        //$('#addDepartment').val('');
                        //$('#addBagian').val('');
                        //$('#addNomorRak').val('');
                        //$('#addLabelRak').val('');
                        $('#addMasaSimpan').val('');
                        $('#addKeterangan').val('');

                        //$('.form-control select2').val('');
                        //$('.select2').val('');

                        //$('.form-control select2').val('');
                        //$("#addBagian").select2('destroy');
                        //$('#addBagian').each(function () {
                        //    $(this).select2({ dropdownParent: $(this).parent() });
                        //});
                        //$("#addBagian").val('- select -');

                        //$("#addBagian").prop('disabled', true);

                        $('#display_Qty_Rak').hide();

                    }
                },
                error: function () {

                }
            });
        });
