    //----------------------------------------------------- APPROVE DOCUMENT -----------------------------------------------------
    function approveData(element) {
        var data = element.dataset.title;
        var Obj = JSON.parse($('#rowData-' + data).val());

        var APPROVED_BY = "";

        if (Obj.DEPT_HEAD_CREATED_SIGN === null) {
            APPROVED_BY = "DEPT_HEAD_USER";
        }
        else if (Obj.ADM_CREATED_SIGN === null) {
            APPROVED_BY = "ADM_CONTROL";
        }

        //Set Data to Approve Popup
        $('#approve_ID').val(Obj.ID).trigger("change");
        $('#approve_NO_DOCUMENT').val(Obj.NO_DOCUMENT).trigger("change");
        $('#approve_APPROVED_BY').val(APPROVED_BY).trigger("change");

        //alert(Obj.ID)

        if (APPROVED_BY == "DEPT_HEAD_USER") {
            if ($('#Approve_3').val() != "True") {
                toastr.warning("Your Account Doesn't Have Access! Please Contact Administrator!");
                return;
            }
            else {
                $('#Confirmation_Approve_Popup').modal();
            }
        }
        else if (APPROVED_BY == "ADM_CONTROL") {
            if ($('#Approve_4').val() != "True") {
                toastr.warning("Your Account Doesn't Have Access! Please Contact Administrator!");
                return;
            }
            else {
                //$('#Confirmation_Approve_Popup').modal();
                $('#modal_approve_ID').val(Obj.ID).trigger("change");

                if (Obj.RAK == "" || Obj.LABEL_RAK == "") {
                    $('#modalApproveForm').modal();
                }
                else {
                    $('#Confirmation_Approve_Popup').modal();
                }

            }
        }

    }

