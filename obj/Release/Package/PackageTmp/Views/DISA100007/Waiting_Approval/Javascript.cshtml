                $('#addLabelRak').val('');

                toastr.warning("Pilih Department!");
                return;
            }

            if (BAGIAN == "" || BAGIAN === null) {
                $('#addLabelRak').val('');

                toastr.warning("Masukkan Bagian!");
                return;
            }

            if (NO_RAK == "" || NO_RAK === null) {
                $('#addLabelRak').val('');

                toastr.warning("Masukkan No. Rak!");
                return;
