    // tombol pencarian data ID Produksi
    function OnSearch(page) {
        searchData(page, $('#paging').val());
        $('#btn_download').prop('disabled', false);
        $('#btn_clear').prop('disabled', false);
    }

    // proses pencarian data ID Produksi
    function searchData(page, size) {
        var JENIS_TRANSAKSI = $('#searchJenisTransaksi').val();
        var NO_DOKUMEN = $('#searchNoDokumen').val();
        var NAMA_DOKUMEN = $('#searchNamaDokumen').val();
        var DEPARTMENT = $('#searchDeptUser').val();
        var STATUS_APPROVE = $('#searchStatusApprove').val();
        var STATUS_DISPOSE = "";
        var NOMOR_RAK = $('#searchNomorRak').val();
        var LABEL_RAK = $('#searchLabelRak').val();
        var TGL_REGISTER = $('#searchTglRegister').val();
        var PAGE_VIEWER = 'WaitingApproval';
        //var CREATED_DATE_FROM = ($('#searchCreatedDateFrom').val()).replace('T', ' ');
        //var CREATED_DATE_TO = ($('#searchCreatedDateTo').val()).replace('T', ' ');

        Last_Record_Per_Page = $("#paging").val();

        $.ajax({
            type: "POST",
            url: SITE_URL + '../Search_Data',
            async: false,
            data: {
                start: page,
                display: size,
                DATA_ID: $("#data_nm").val(),
                EXECUTION_TIME: $("#exec_time_cd").val(),
                TIME_UNIT_CRITERIA: $("#time_unit_cd").val(),
                STATUS_ACTIVE: $("#sts_cd").val(),
                JENIS_TRANSAKSI: JENIS_TRANSAKSI,
                NO_DOKUMEN: NO_DOKUMEN,
                NAMA_DOKUMEN: NAMA_DOKUMEN,
                DEPARTMENT: DEPARTMENT,
                STATUS_APPROVE: STATUS_APPROVE,
                STATUS_DISPOSE: STATUS_DISPOSE,
                NOMOR_RAK: NOMOR_RAK,
                LABEL_RAK: LABEL_RAK,
                TGL_REGISTER: TGL_REGISTER,
                PAGE_VIEWER: PAGE_VIEWER
            },
            success: function (data) {
                $("#GridTable").html(data);
                $("#paging").val(Last_Record_Per_Page);
                if ($('#cntPage').val() == "1") {
                    $('#btn_clear').prop('disabled', false);
                } else {
                    $('#btn_clear').prop('disabled', false);
                }

                scroll_and_checkbox();
                cek_perdata();
            }
        });
    }

