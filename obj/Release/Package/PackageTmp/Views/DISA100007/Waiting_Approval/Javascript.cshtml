    //-------------------Show Change Jenis Transaksi---------------------
    function changeJenisTransaksi() {
        $('.select2').select2({ width: '100%' });
        
        $('#addJenisTransaksi').change(function () {
            var jenis_transaksi = $('#addJenisTransaksi').val();
            if (jenis_transaksi == "pinjam") {
                $('#display_selectNamaDokumen').show();
