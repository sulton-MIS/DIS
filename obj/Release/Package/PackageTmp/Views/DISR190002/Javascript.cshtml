                            jsWebClientPrint.print('&PRINTER_NAME=' + PRINTER_NAME + '&Pathfile=' + Pathfile);
                        }
                    }

                    var delayWaiting = 5000; //5 second

                    //Delete Files from Directory after 'delayWaiting' ...
                    const await = new Promise(resolve => {
                        setTimeout(() => {
                            resolve(deleteFiles()); //Delete files temp
                        }, delayWaiting);
                    });

                    await.then(() => await);

                    toastr.success('Berhasil print!');
                    //clearForm();

                } else {
