    //event auto search
    function eventAutoSearchTyping() {
        var ID_SEISAN = document.getElementById("searchIDSeisan").value;
        if (ID_SEISAN.length < 17 && ID_SEISAN.length > 20 && !ID_SEISAN.includes("S")) {
            toastr.warning("Please input the correct ID Produksi!!");
            //$('#searchIDSeisan').val('');
            //OnSearch(1);

        } else {
            OnSearch(1);

            var ID_SEISAN = $('#searchIDSeisan').val();

            var ID_HINMOKU = "";
            var OTHER_LOTNO = "";

            $.ajax({
                url: SITE_URL + 'get_Tipe',
                data: {
                    ID_SEISAN: ID_SEISAN.trim()
                },
                success: function (e) {
                    if (e.data.length > 0) {
                        ID_HINMOKU = e.data[0].ID_HINMOKU;
                        OTHER_LOTNO = e.data[0].OTHER_LOTNO;

                        $('#editIDHinmoku').val(ID_HINMOKU);
                        $('#editLot').val(OTHER_LOTNO);

                        if (ID_HINMOKU.includes("-T")) {
                            $('#Display_GlassBuwon').hide();
                            $('#Display_LotIhouse').show();
                            $('#editLotIhouse').focus();
                        } else {
                            $('#Display_GlassBuwon').show();
                            $('#Display_LotIhouse').hide();
                        }
                    }
                },
                error: function () {

                }
            });
        }
    }

    //search by Enter
    function enterKey_Press() {
        $('input').bind("enterKey", function (e) {
            if ($('#searchIDSeisan').val() == "") {
                toastr.warning("PLEASE INPUT ID PRODUKSI!");
            } else {
                OnSearch(1);

                var ID_SEISAN = $('#searchIDSeisan').val();

                var ID_HINMOKU = "";
                var OTHER_LOTNO = "";

                $.ajax({
                    url: SITE_URL + 'get_Tipe',
                    data: {
                        ID_SEISAN: ID_SEISAN.trim()
                    },
                    success: function (e) {
                        if (e.data.length > 0) {
                            ID_HINMOKU = e.data[0].ID_HINMOKU;
                            OTHER_LOTNO = e.data[0].OTHER_LOTNO;

                            $('#editIDHinmoku').val(ID_HINMOKU);
                            $('#editLot').val(OTHER_LOTNO);

                            if (ID_HINMOKU.includes("-T")) {
                                $('#Display_GlassBuwon').hide();
                                $('#Display_LotIhouse').show();
                                $('#Display_LotIhouse').focus();
                            } else {
                                $('#Display_GlassBuwon').show();
                                $('#Display_LotIhouse').hide();
                            }
                        }
                    },
                    error: function () {

                    }
                });
                
            }
        });
        $('input').keyup(function (e) {
            if (e.keyCode == 13) {
                $(this).trigger("enterKey");
            }
        });
    }

