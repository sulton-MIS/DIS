    // proses pencarian data ID Produksi
    function searchData(page, size) {
        var ID_SEISAN = $('#searchIDSeisan').val();
        var ID_HINMOKU = $('#searchItemCode').val();

        if (ID_SEISAN != "") {
            var ID_SEISAN_LENGTH = ID_SEISAN.replace(/ /g, '').length;

            if (ID_SEISAN_LENGTH > 15) {
                Last_Record_Per_Page = $("#paging").val();
                $.ajax({
                    type: "POST",
                    url: SITE_URL + 'Search_Data',
                    async: false,
                    data: {
                        start: page,
                        display: size,
                        DATA_ID: $("#data_nm").val(),
                        EXECUTION_TIME: $("#exec_time_cd").val(),
                        TIME_UNIT_CRITERIA: $("#time_unit_cd").val(),
                        STATUS_ACTIVE: $("#sts_cd").val(),
                        ID_SEISAN: ID_SEISAN
                        , ID_HINMOKU: ID_HINMOKU

                    },
                    success: function (data) {
                        $("#Datagrid_Data").html(data);
                        $("#paging").val(Last_Record_Per_Page);
                        if ($('#cntPage').val() == "1") {
                            $('#btn_clear').prop('disabled', false);
                        } else {
                            $('#btn_clear').prop('disabled', false);
                        }

                        scroll_and_checkbox();
                        cek_perdata();
                    }
                });
            }
            //else {
            //    toastr.warning('PLEASE INPUT ID PRODUKSI CORRECTLY!')
            //}
        }
        else {
            toastr.warning("PLEASE INPUT ID PRODUKSI!");
        }


    }

