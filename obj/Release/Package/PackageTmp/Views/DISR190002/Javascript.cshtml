    // proses pencarian data ItemCode (Similar Type)
    function searchDataItemCode(page, size) {
        var CODE = $('#searchItemCode').val();

        if (CODE != "") {
            Last_Record_Per_Page = $("#paging").val();
            $.ajax({
                type: "POST",
                url: SITE_URL + 'Search_Data_ItemCode',
                async: false,
                data: {
                    start: page,
                    display: size,
                    DATA_ID: $("#data_nm").val(),
                    EXECUTION_TIME: $("#exec_time_cd").val(),
                    TIME_UNIT_CRITERIA: $("#time_unit_cd").val(),
                    STATUS_ACTIVE: $("#sts_cd").val(),
                    CODE: CODE

                },
                success: function (data) {
                    $("#GridTable").html(data);
                    $("#paging").val(Last_Record_Per_Page);
                    if ($('#cntPage').val() == "1") {
                        $('#btn_clear').prop('disabled', false);
                    } else {
