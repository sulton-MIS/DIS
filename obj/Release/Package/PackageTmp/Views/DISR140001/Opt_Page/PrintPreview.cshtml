                                <td class="tg-0pky" colspan="2">@ViewData["NAMA"]</td>
                                <td class="tg-0pky"></td>
                                <td class="tg-0pky"></td>
                                <td class="tg-0pky"></td>
                                <td class="tg-c3ow"></td>
                            </tr>
                            <tr>
