                                text-align: center;
                                vertical-align: middle
                            }

                            .tg .tg-fymr {
                                border-color: inherit;
                                font-weight: bold;
                                text-align: left;
                                vertical-align: top
                            }

                            .tg .tg-0pky {
                                border-color: inherit;
                                text-align: left;
                                vertical-align: top
                            }
