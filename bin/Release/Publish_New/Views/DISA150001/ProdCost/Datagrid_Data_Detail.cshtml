                                        var number = 1;
                                        foreach (DISA150001MaterialCost item in Material)
                                        {
                                    <tr id="row-@number" class="row-@item.ID">
                                        <td class="align-center align-middle" style="width:30px"> @item.ROW_NUM </td>
                                        <td class="align-left align-middle" style="width:150px"> @item.DMC_CODE </td>
                                        <td class="align-left align-middle" style="width:40px"> @item.PART </td>
