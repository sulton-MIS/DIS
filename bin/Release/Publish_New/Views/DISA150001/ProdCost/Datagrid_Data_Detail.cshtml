                <div style="border: 1pt solid #ccc; overflow:scroll; position: relative;">
                    <div id="gutterGrid" style="position: absolute; display: block; top: 0; right: 0; width: 17px; height: auto; border-left: 0px solid #ddd; border-top: 1px solid #ddd; border-bottom: 1px solid #ddd; background: rgb(241, 241, 241);"></div>
                    <div id="tScrollHead" style="width: 100%; border-right:17px solid #fff; position:sticky; top:0">
                        <table id="dynamic-table" class="table table-striped table-bordered table-condensed table-fixed" style="min-width: 100%; table-layout: fixed">
                            <thead>
                                <tr>
                                    <th style="width:30px">NO.</th>
                                    <th style="width:100px">PART</th>
                                    <th style="width:180px">DMC CODE PARTS</th>
                                    <th>KODE PROSES</th>
                                    <th style="width:180px">NAMA PROSES</th>
                                    <th>SETTING TIME (sec)</th>
                                    <th>CYCLE TIME (sec)</th>
                                    <th>LOT SIZE</th>
                                    <th>TOTAL TIME</th>
                                    <th>PROD YIELD</th>
                                    <th>CHINRITSU</th>
                                    <th>CAVITY</th>
                                    <th>PRICE/Sheet</th>
                                    <th>PRICE/Pcs</th>
                                </tr>
                            </thead>
                            @{
                            if (Material_Usage == null || Material_Usage.Count == 0)
                            {
                                <tr id="NoDataFound">
                                    <td colspan="10" class="text-center">No data found</td>
                                </tr>
                            }
                            }
                        </table>
                    </div>

                    <!--data grid-->
                    <div id="tScrollBody" style="height: 350px;">
                        <table id="tblScroll" class="table table-bordered table-condensed table-fixed" style="min-width: 100%; table-layout: fixed; border: none">
                            <tbody id="tableBody">

                                @{
                                if (Material_Usage != null)
                                {
                                    if (Material_Usage.Count > 0)
                                    {
                                        var number = 1;
                                        foreach (DISA150001MaterialCostUsage item in Material_Usage)
                                        {
                                    <tr id="row-@number" class="row-@item.ID">
                                        <td style="width:30px" class="align-center align-middle"> @item.ROW_NUM </td>
                                        <td style="width:100px"> @item.PART </td>
                                        <td style="width:180px"> @item.DMC_CODE_PARTS </td>
                                        <td class="align-center align-middle"> @item.KODE_PROSES </td>
                                        <td style="width:180px"> @item.NAMA_PROSES </td>
                                        <td class="align-center align-middle"> @item.SETTING_TIME </td>
                                        <td class="align-center align-middle"> @item.CYCLE_TIME </td>
                                        <td class="align-center align-middle"> @item.LOT_SIZE </td>
                                        <td class="align-center align-middle"> @item.TOTAL_TIME </td>
                                        <td> @item.PROD_YIELD </td>
                                        <td> @item.CHINRITSU </td>
                                        <td class="align-center align-middle"> @item.CAVITY </td>
                                        <td> @item.PRICE_PER_SHEET </td>
                                        <td> @item.PRICE_PER_PCS </td>
                                    </tr>
                                            number++;
                                        }
                                    }
                                }
                                }
                                <!----><!----><!---->
                            </tbody>
                        </table>
                    </div>
                </div>
                <table class="table table-striped table-bordered table-condensed table-fixed" style="width: 100%">
                    <tfoot>
                        <tr>
                            <td colspan="8" id="partialPaging">
                                @Html.Partial("ProdCost/Datagrid_Paging")
                            </td>
                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
        <div class="col align-right align-bottom">
            <button type="button" class="btn btn-danger btn-sm btn-close" id="btn_close" onclick="back_summary()"><i class="fa fa-close"></i></button>
        </div>
    </div>
    </div>
    @Html.Partial("ProdCost/Javascript_detail")
