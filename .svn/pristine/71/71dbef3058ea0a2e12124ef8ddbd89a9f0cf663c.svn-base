                            <th style="width: 150px;" rowspan="2">IDENTITY NO</th>
                            <th style="width: 150px;" rowspan="2">PIC STATUS</th>
                            <th style="width: 180px;" rowspan="2">SAFETY INDUCTION NO</th>
                            <th style="width: 180px;" rowspan="2">SAFETY INDUCTION FROM</th>
                            <th style="width: 180px;" rowspan="2">SAFETY INDUCTION TO</th>
                            <th style="width: 150px;" rowspan="2">DETAIL TRAINING</th>
                            <th style="width: 150px;" rowspan="2">DETAIL SCORE</th>
                        </tr>
                    </thead>
                    <tr>
                        <td style="width: 60px">
                        </td>
