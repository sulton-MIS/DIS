DECLARE @@CNT INT
	, @@CHK VARCHAR(20)
	, @@ERR VARCHAR(MAX);

DECLARE @@COUNTER INT= (SELECT (CASE WHEN (SELECT COUNT(PROCESS_ID) FROM TB_R_LOG_H) = 0 THEN 1 ELSE (SELECT TOP 1 PROCESS_ID FROM TB_R_LOG_H ORDER BY PROCESS_ID DESC) + 1 END));
EXEC SP_INSERT_UPDATE_LOG_H @USERNAME,'','WP01006',1,'1','Update',@@COUNTER;

DECLARE @@MSG_START VARCHAR(MAX) = 'LOG001 : '+ (SELECT dbo.GenerateMessage('LOG001','','',''));
EXEC SP_INSERT_LOG_D @@COUNTER, 'Start', 'LOG001', 'Info',  @@MSG_START;

BEGIN TRY
	UPDATE TB_M_COMPANY SET COMPANY_CODE = @COMPANY_CODE, COMPANY_NAME = @COMPANY_NAME, COMPANY_INITIAL = @COMPANY_INITIAL, CONTACT_PERSON = @CONTACT_PERSON, EMAIL = @EMAIL, ADDRESS = @ADDRESS, PHONE = @PHONE, STATUS = 1, FLAG = @FLAG, UPDATED_BY = @USERNAME, UPDATED_DT = GETDATE() WHERE ID_TB_M_COMPANY = @ID
	SET @@CHK = 'TRUE';
	SET @@ERR = 'Data Has Been Updated';
	DECLARE @@MSG_SUCCESS VARCHAR(MAX) = 'MWP003 : '+ (SELECT dbo.GenerateMessage('MWP003','COMPANY','',''));
	EXEC SP_INSERT_LOG_D @@COUNTER, 'Updated', 'MWP003', 'Info',  @@MSG_SUCCESS;

END TRY
BEGIN CATCH
	SET @@CHK = 'FALSE';
	SET @@ERR = 'ERROR UPDATE TB_M_COMPANY:' +@COMPANY_NAME+
	'<br/>Detail Error :|: ' + ERROR_MESSAGE() + ' :|: ';	
END CATCH

SELECT @@CHK AS STACK, @@ERR AS LINE_STS

DECLARE @@MSG_END VARCHAR(MAX) = 'LOG007 : '+ (SELECT dbo.GenerateMessage('LOG007','','',''));
EXEC SP_INSERT_LOG_D @@COUNTER, 'END', 'LOG007', 'Info',  @@MSG_END;

EXEC SP_INSERT_UPDATE_LOG_H 
@USERNAME,'','WP01006','1',1,'CREATE',@@COUNTER;