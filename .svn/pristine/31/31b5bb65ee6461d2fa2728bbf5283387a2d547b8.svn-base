<div id="gridlookup" class="modal fade"> 
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="popup-title">Grid Lookup</h4>
            </div>

            <div class="modal-body">
                <div class="row ">
                    <div class="col-xs-12 col-md-12">
                        <div class="form-group form-group-xs">
                            <div>
                                <div class="input-group input-group-xs">
                                    <input id="txtPopupSearchLookup" class="form-control" type="text" placeholder="input name...." value="" >
                                    <span class="input-group-btn">
                                        <button type="button" id="btnsearchlookupuser" 
                                            data-loading-text="<i class='glyphicon-refresh-animate glyphicon glyphicon-refresh'></i>"
                                            class="btn btn-default btn-sm" style="line-height: 1.1; top: -1px; left: 5px;">&nbsp<i class="fa fa-search"></i>
                                        </button> 
                                        <button onclick="javascript:clearPopupFilter2()" id="btnclearlookup" class=" btn btn-default btn-sm " title="Clear" type="button" style="line-height: 1.1; top: -1px; left: 10px; width: 43px;">&nbsp<i class="fa  fa-eraser"></i>&nbsp</button>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="form-group form-group-xs" >
                           <div class="table-responsive">
                                <table class="table table-striped table-bordered table-condensed">
                                    <thead>
                                        <tr>
                                            <th class="text-center" style="width: 50px;">
                                                <input class="popupLookup-checkbox cursor-link" type="checkbox" style="height: 13px; margin-top: 3px;" id="popupLookup-checkAll" />
                                            </th>
                                            <th class="text-center">Field 1</th>
                                            <th class="text-center">Field 2</th>
                                            <th class="text-center">Field 3</th>
                                            <th class="text-center">Field 4</th>
                                            <th class="text-center" style="width: 17px;">&nbsp</th>
                                        </tr>
                                    </thead>
    
                                    @for (int i = 1; i <= 8; i++)
                                    {
                                        <tr>
                                            <td class="text-center" style="width: 50px;">
                                                <input type="checkbox" class="popupLookup-checkbox popupLookup-checkbox-body cursor-link" style="height: 13px; margin-top: 3px;" data-value="Data-@i" />
                                            </td>
                                            <td class="text-center">Data-@i</td>
                                            <td class="text-center">Data-@i</td>
                                            <td class="text-center">Data-@i</td>
                                            <td class="text-center">Data-@i</td>
                                        </tr>
                                    }
                                </table>
                            </div> 
                        </div>
                    </div>
                </div> 
            </div> 
            <div class="modal-footer"> 
                <button type="button" class="btn btn-primary btn-xs" style="width: 70px;" onclick="javascript:popuplookupSelected('OK')" data-dismiss="modal">Ok</button>
                <button type="button" class="btn btn-danger btn-xs" style="width: 70px;" onclick="javascript:popuplookupSelected('CS')" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>