IF (@TYPE = 'WORKING')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_WORKING
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'SHIFT')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_SHIFT
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'IDENTIFICATION')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_IDENTIFICATION
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'IMPACT')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_IMPACT
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'SUPERVISION')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_SUPERVISION
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'HENKATEN_ENV')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_HENKATEN_ENV
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END
ELSE IF (@TYPE = 'HENKATEN_SAFETY')
BEGIN
	SELECT	* 
	FROM	TB_R_WP_DETAIL_HENKATEN_SAFETY
	WHERE	WP_PROJECT_JOB_ID = @WP_PROJECT_JOB_ID
END